{
  "buildCommand": "./vercel-build.sh",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "NEXT_DISABLE_TYPECHECK": "1",
    "NODE_OPTIONS": "--max-old-space-size=4096"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxLambdaSize": "15mb",
        "memory": 3008
      }
    }
  ]
}
